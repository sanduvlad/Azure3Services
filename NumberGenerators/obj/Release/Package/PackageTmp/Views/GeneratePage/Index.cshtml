@model NumberGenerators.Models.Model


@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<h6>Wellcome @(Model.generator.UserName)</h6>

@using (Html.BeginForm("GenerateNumbers", "GeneratePage", new { guid = Model.generator.ID }))
{
    <input type="submit" value="Generate!" />
}

 
    @if (Model.numberList != null && Model.numberList.Count > 0)
    {
        for (int i = 0; i < Model.numberList.Count; i++)
        {
        <p>@(Model.numberList[i].Number)</p>
        }
    }